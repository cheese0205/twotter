AA,S,GAAS;AAAA;GACpC,CAFN,CAGI,CARN,CAUI,EADJ,YAAA,CAAyC,2BAAzC,EAAyC;AAAtB,IAAA,UAAS,EAAE,QAAA,CAAA;AAAW,GAAzC,EAAsC,IAAtC,EAAsC;AAAA;AAAtC,IAAsC,CAAA,YAAA,CAAtC,CACI,CAXN,CAoBI,EARJ,YAAA,CAOM,KAPN,EAAA,UAAA,EAOM,E,kBANJ,YAAA,CAKE,SALF,EAKE,IALF,EAKE,WAAA,CAJgB,KAAA,CAAA,IAAA,CAAK,MAIrB,EAJ2B,UAApB,KAAoB,EAAf;yBADd,YAAA,CAKE,oBALF,EAKE;AAHC,MAAA,GAAG,EAAE,KAAK,CAAC,EAGZ;AAFC,MAAA,QAAQ,EAAE,KAAA,CAAA,IAAA,CAAK,QAEhB;AADC,MAAA,KAAK,EAAE;AACR,KALF,E,IAAA,E;;AAAA,M,qBAAA,C;GAKE,CALF,E;;AAAA,GAMI,EAPN,CAQI,CArBN,C","sourcesContent":["<template>\n  <div class=\"user-profile\">\n    <div class=\"user-profile__sidebar\">\n      <div class=\"user-profile__user-panel\">\n        <h1 class=\"user-profile__username\">@{{ user.username }}</h1>\n        <div class=\"user-profile__admin-badge\" v-if=\"user.isAdmin\">\n          Admin\n        </div>\n        <div class=\"user_prodile__follower-count\">\n          <strong>Followers: </strong>{{ followers }}\n        </div>\n      </div>\n      <CreateTwootPanel @add-twoot=\"addTwoot\"/>\n    </div>\n    <div class=\"user-profile__twoots-wrapper\">\n      <TwootItem\n        v-for=\"twoot in user.twoots\"\n        :key=\"twoot.id\"\n        :username=\"user.username\"\n        :twoot=\"twoot\"\n      />\n    </div>\n  </div>\n</template>\n\n<script>\nimport TwootItem from \"./Twootitem\";\nimport CreateTwootPanel from \"./CreateTwootPanel\";\n\nexport default {\n  name: 'UserProfile',\n  components: {TwootItem, CreateTwootPanel},\n  data(){\n    return {\n      followers: 0,\n      user: {\n        id: 1,\n        username: '_CheeseGaga',\n        firstname: 'Cheese',\n        lastname: 'Gaga',\n        email: 'cheese000205@g mail.com',\n        isAdmin: true,\n        twoots: [\n          { id:1, content: 'Twotter is Amazing!'},\n          { id:2, content: \"Don't forget to subscribe!\"}\n        ]\n      }\n    }\n  },\n  methods: {\n    addwoot(twoot) {\n      this.user.twoots.unshift( {\n        id: this.user.twoots.length + 1,\n        content: twoot\n      })\n    },\n    addTwoot(twoot) {\n      this.user.twoots.unshift( {\n        id: this.user.twoots.length + 1,\n        content: twoot\n      });\n    }\n  }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n.user-profile {\n  display: grid;\n  grid-template-columns: 1fr 3fr;\n  grid-gap: 50px;\n  padding:  50px 5%;\n\n  .user-profile__user-panel {\n    display: flex;\n    flex-direction: column;\n    padding: 20px;\n    background-color: white;\n    border-radius: 5px;\n    border: 1px solid #DFE3E8;\n    margin-bottom: auto;\n\n    h1 {\n      margin: 0;\n    }\n\n    .user-profile__admin-badge {\n      background: deepskyblue;\n      color: white;\n      border-radius: 5px;\n      margin-right: auto;\n      padding: 0 10px;\n      font-weight: bold;\n    }\n  }\n  .user-profile__twoots-wrapper {\n    display: grid;\n    grid-gap: 10px;\n    margin-bottom: auto;\n  }\n}\n</style>\n"],"sourceRoot":""}]}