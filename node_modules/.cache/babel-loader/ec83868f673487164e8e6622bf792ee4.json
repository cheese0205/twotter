D,CAAzB,E;;AAAA,C;;+BAQF,YAAA,CAES,QAFT,EAES,IAFT,EAAQ,UAAR,EAEA,CAAA;AAAA;AAFA,C;;;AAKC,EAAA,KAAK,EAAC;;;;;uBA1Bb,YAAA,CAmCM,KAnCN,EAAA,UAAA,EAmCM,CAlCJ,YAAA,CAwBM,KAxBN,EAAA,UAAA,EAwBM,CAvBJ,YAAA,CAA4D,IAA5D,EAAA,UAAA,EAAmC,MAAC,gBAAA,CAAG,KAAA,CAAA,IAAA,CAAK,QAAR,CAApC,EAAoD;AAAA;AAApD,GAuBI,EAtByC,KAAA,CAAA,IAAA,CAAK,O,kBAAlD,YAAA,CAEM,KAFN,EAAA,UAAA,EAA2D,SAA3D,C,qCAsBI,EAnBJ,YAAA,CAEM,KAFN,EAAA,UAAA,EAEM,CADJ,UACI,E,kCAD2B,KAAA,CAAA,S,GAAS;AAAA;GACpC,CAFN,CAmBI,EAhBJ,YAAA,CAeO,MAfP,EAeO;AAfD,IAAA,KAAK,EAAC,4BAeL;AAfmC,IAAA,QAAM,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA,cAAA,CAAA;AAAA,aAAU,QAAA,CAAA,cAAA,OAAA,QAAA,YAAV;AAAA,KAAA,EAAwB,CAAA,SAAA,CAAxB,CAAA;AAezC,GAfP,E,CACE,U,kBACA,YAAA,CAA4D,UAA5D,EAA4D;AAAlD,IAAA,EAAE,EAAC,UAA+C;AAApC,IAAA,IAAI,EAAC,GAA+B;;aAAlB,KAAA,CAAA,eAAA,GAAe,M;;AAAG,GAA5D,E,IAAA,E;;AAAA,G,iBAA0C,KAAA,CAAA,e,KAC1C,YAAA,CAOM,KAPN,EAAA,UAAA,EAOM,CANJ,UAMI,E,gBALJ,YAAA,CAIS,QAJT,EAIS;AAJD,IAAA,EAAE,EAAC,cAIF;;aAJ0B,KAAA,CAAA,iBAAA,GAAiB,M;;AAI3C,GAJT,E,oBACE,YAAA,CAES,SAFT,EAES,IAFT,EAES,WAAA,CAF+C,KAAA,CAAA,UAE/C,EAFyD,UAA5B,MAA4B,EAApB,KAAoB,EAAf;yBAAnD,YAAA,CAES,QAFT,EAES;AAFA,MAAA,KAAK,EAAE,MAAM,CAAC,KAEd;AAF4D,MAAA,GAAG,EAAE;AAEjE,KAFT,E,iBACK,MAAM,CAAC,I,CADZ,EACgB;AAAA;AADhB,MACgB,CAAA,OAAA,CADhB,C;GAES,CAFT,E;;AAAA,G,EADF,E;;AAAA,G,mBAAmC,KAAA,CAAA,iB,GAK/B,CAPN,C,EASA,W,CAZF,E;;AAAA,GAgBI,CAxBN,CAkCI,EATJ,YAAA,CAQM,KARN,EAAA,WAAA,EAQM,E,kBAPL,YAAA,CAME,SANF,EAME,IANF,EAME,WAAA,CALgB,KAAA,CAAA,IAAA,CAAK,MAKrB,EAL2B,UAApB,KAAoB,EAAf;yBADd,YAAA,CAME,oBANF,EAME;AAJC,MAAA,GAAG,EAAE,KAAK,CAAC,EAIZ;AAHC,MAAA,QAAQ,EAAE,KAAA,CAAA,IAAA,CAAK,QAGhB;AAFC,MAAA,KAAK,EAAE,KAER;AADC,MAAA,WAAS,EAAE,QAAA,CAAA;AACZ,KANF,E,IAAA,E;;AAAA,M,oCAAA,C;GAME,CANF,E;;AAAA,GAOK,EARN,CASI,CAnCN,C","sourcesContent":["<template>\n  <div class=\"user-profile\">\n    <div class=\"user-profile__user-panel\">\n      <h1 class=\"user-profile__username\">@{{ user.username }}</h1>\n      <div class=\"user-profile__admin-badge\" v-if=\"user.isAdmin\">\n        Admin\n      </div>\n      <div class=\"user_prodile__follower-count\">\n        <strong>Followers: </strong>{{ followers }}\n      </div>\n      <form class=\"user-profile__create-twoot\" @submit.prevent=\"createNewTwoot\">\n        <label for=\"newTwoot\"><strong>New Twoot</strong></label>\n        <textarea id=\"newTwoot\" rows=\"4\" v-model=\"newTwootContent\"/>\n        <div class=\"user-profile__create-twoot-type\">\n          <label for=\"newTwootType\"><strong>Type: </strong></label>\n          <select id=\"newTwootType\" v-model=\"selectedTwootType\">\n            <option :value=\"option.value\" v-for=\"(option, index) in twootTypes\" :key=\"index\">\n              {{ option.name }}\n            </option>\n          </select>\n        </div>\n\n        <button>\n          Twoot!\n        </button>\n      </form>\n    </div>\n    <div class=\"user-profile__twoots-wrapper\">\n     <TwootItem\n       v-for=\"twoot in user.twoots\"\n       :key=\"twoot.id\"\n       :username=\"user.username\"\n       :twoot=\"twoot\"\n       @favourite=\"toggleFavourite\"\n     />\n    </div>\n  </div>\n</template>\n\n<script>\nimport TwootItem from \"./Twootitem\";\n\nexport default {\n  name: 'UserProfile',\n  components: {TwootItem},\n  data(){\n    return {\n      followers: 0,\n      user: {\n        id: 1,\n        username: '_CheeseGaga',\n        firstname: 'Cheese',\n        lastname: 'Gaga',\n        email: 'cheese000205@gmail.com',\n        isAdmin: true,\n        twoots: [\n          { id:1, content: 'Twotter is Amazing!'},\n          { id:2, content: \"Don't forget to subscribe!\"}\n        ]\n      },\n      twootTypes: [\n        { value: 'draft', name: 'Draft'},\n        { value: 'instant', name: 'Instant Twoot'}\n      ],\n      newTwootContent: '',\n      selectedTwootType: 'instant'\n    }\n  },\n  watch: {\n    followers(newFollowerCount, oldFollowerCount) {\n      if (oldFollowerCount < newFollowerCount) {\n        console.log(`${this.user.username} has gained a new follower!`)\n      }\n    }\n  },\n  computed: {\n    fullName(){\n      return `${this.user.firstname} ${this.user.lastname}`\n    }\n  },\n  methods: {\n    followUser(){\n      this.followers++\n    },\n    toggleFavourite(id) {\n      console.log(`Favourite Twoot #${id}`)\n    },\n    createNewTwoot() {\n      if (this.newTwootContent && this.selectedTwootType !== 'draft') {\n        this.user.twoots.unshift( {\n          id: this.user.twoots.length + 1,\n          content: this.newTwootContent\n        })\n      }\n      this.newTwootContent = ''\n    }\n  },\n  mounted() {\n    this.followUser()\n  }\n}\n</script>\n\n<style>\n.user-profile {\n  display: grid;\n  grid-template-columns: 1fr 3fr;\n  width: 100%;\n  padding:  50px 5%;\n}\n\n.user-profile__user-panel {\n  display: flex;\n  flex-direction: column;\n  margin-right: 50px;\n  padding: 20px;\n  background-color: white;\n  border-radius: 5px;\n  border: 1px solid #DFE3E8;\n}\n\n.user-profile__admin-badge {\n  background: deepskyblue;\n  color: white;\n  border-radius: 5px;\n  margin-right: auto;\n  padding: 2px 5px;\n  font-weight: bold;\n}\n\nh1 {\n  margin: 0;\n}\n\n.user-profile__twoots-wrapper {\n  display: grid;\n  grid-gap: 10px;\n}\n\n.user-profile__create-twoot {\n  border-top: 1px solid #DFE3E8;\n  padding-top: 20px;\n  display: flex;\n  flex-direction: column;\n}\n</style>\n"],"sourceRoot":""}]}